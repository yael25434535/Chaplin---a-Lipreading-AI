      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "NGW2TVN6YvvR"
      },
      "outputs": [],
      "source": [
        "model.compile(optimizer=Adam(learning_rate=0.0001), loss=CTCLoss)\n"
      ]
    },
